    "@types/node": "^17.0.23",
    "body-parser": "^1.19.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "mysql": "^2.18.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
